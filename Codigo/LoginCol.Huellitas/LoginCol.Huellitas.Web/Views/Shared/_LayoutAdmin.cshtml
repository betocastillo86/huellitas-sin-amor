    <link rel="shortcut icon" href="img/favicon.ico">

    
    
</head>

<body>
    <!-- topbar starts -->
    <div class="navbar navbar-default" role="navigation">

        <div class="navbar-inner">
            <button type="button" class="navbar-toggle pull-left animated flip">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="index.html">
                <img alt="Charisma Logo" src="/Images/Admin/logo20.png" class="hidden-xs" />
                <span>Charisma</span>
            </a>

            <!-- user dropdown starts -->
            <div class="btn-group pull-right">
                <button class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                    <i class="glyphicon glyphicon-user"></i><span class="hidden-sm hidden-xs"> @LoginCol.Huellitas.Web.Models.SessionModel.Usuario.Nombres</span>
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    <li><a href="#">Profile</a></li>
                    <li class="divider"></li>
                    <li><a href="/admin/salir">Salir</a></li>
                </ul>
            </div>
            <!-- user dropdown ends -->
            <!-- theme selector starts -->
            <div class="btn-group pull-right theme-container animated tada">
                <button class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                    <i class="glyphicon glyphicon-tint"></i><span class="hidden-sm hidden-xs">Change Theme / Skin</span>
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu" id="themes">
                    <li><a data-value="classic" href="#"><i class="whitespace"></i> Classic</a></li>
                    <li><a data-value="cerulean" href="#"><i class="whitespace"></i> Cerulean</a></li>
                    <li><a data-value="cyborg" href="#"><i class="whitespace"></i> Cyborg</a></li>
                    <li><a data-value="simplex" href="#"><i class="whitespace"></i> Simplex</a></li>
                    <li><a data-value="darkly" href="#"><i class="whitespace"></i> Darkly</a></li>
                    <li><a data-value="lumen" href="#"><i class="whitespace"></i> Lumen</a></li>
                    <li><a data-value="slate" href="#"><i class="whitespace"></i> Slate</a></li>
                    <li><a data-value="spacelab" href="#"><i class="whitespace"></i> Spacelab</a></li>
                    <li><a data-value="united" href="#"><i class="whitespace"></i> United</a></li>
                </ul>
            </div>
            <!-- theme selector ends -->

            <ul class="collapse navbar-collapse nav navbar-nav top-menu">
                <li><a href="/"><i class="glyphicon glyphicon-globe"></i> Visit Site</a></li>
            </ul>

        </div>
    </div>
    <!-- topbar ends -->
    <div class="ch-container">
        <div class="row">

            <!-- left menu starts -->
            <div class="col-sm-2 col-lg-2">
                <div class="sidebar-nav">
                    <div class="nav-canvas">
                        <div class="nav-sm nav nav-stacked">

                        </div>
                        <script type="text/template" id="templateMenuIzq">
                            <% if (typeof(EsHeader) != "undefined" && EsHeader == true) { %>
                               <li class="nav-header"><%= Nombre %></li> 
                            <% } 
                            else {
                            %>
                                <a class="ajax-link" href="<%= Vinculo %>"><i class="glyphicon glyphicon-home"></i><span> <%= Nombre %></span></a>
                            <%
                                }
                            %>
                        </script>
                        <ul class="nav nav-pills nav-stacked main-menu" id="nav-menu-izq">
                            <li class="nav-header">Menú</li>
                        </ul>
                    </div>
                </div>
            </div>
            <!--/span-->
            <!-- left menu ends -->
            
            <noscript>
                <div class="alert alert-block col-md-12">
                    <h4 class="alert-heading">Warning!</h4>

                    <p>
                        You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a>
                        enabled to use this site.
                    </p>
                </div>
            </noscript>

            <div id="content" class="col-lg-10 col-sm-10">
                <!-- content starts -->
              
                <div id="appAdminContent">
                    @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "form1", name = "form1", enctype = "multipart/form-data" }))
                    { 
                        @RenderBody()
                    }

                    
                </div>


                <!-- content ends -->
            </div><!--/#content.col-md-0-->
        </div><!--/fluid-row-->

        <hr>

        @Html.Partial("~/Views/Shared/_ModalAlertAdmin.cshtml")
        @Html.Partial("~/Views/Shared/_ModalHtmlAdmin.cshtml")

        <footer class="row">
            <p class="col-md-9 col-sm-9 col-xs-12 copyright">
                &copy; <a href="http://usman.it" target="_blank">
                    Login Colombia
                </a> 2014
            </p>

            <p class="col-md-3 col-sm-3 col-xs-12 powered-by">
                Powered by: <a href="#">Login Colombia</a>
            </p>
        </footer>

    </div><!--/.fluid-container-->
    <!-- external javascript -->
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/calendar")
    @Scripts.Render("~/bundles/chosen")
    @Scripts.Render("~/bundles/colorbox")
    @Scripts.Render("~/bundles/jqueryTemplate")
    
    
    @Scripts.Render("~/bundles/huellitas")
    

    
    <script src="~/Scripts/huellitas.admin.router.js"></script>
    <script src="~/Scripts/huellitas.admin.app.js"></script>


    
    
    <script src="~/Scripts/models/huellitas.admin.Admin.js"></script>
    <script src="~/Scripts/views/huellitas.admin.AdminView.js"></script>
    <script src="~/Scripts/models/huellitas.ZonaGeografica.js"></script>
    <script src="~/Scripts/views/huellitas.admin.AlertView.js"></script>
    <script src="~/Scripts/views/huellitas.admin.AlertHtmlView.js"></script>
    <script src="~/Scripts/models/huellitas.admin.Usuario.js"></script>
    <script src="~/Scripts/libs/backbone.stickit.js"></script>
    <script src="~/Scripts/libs/backbone-validation.js"></script>
    @RenderSection("scripts", required: false)
    
    
    


    
    
    @*<!-- select or dropdown enhancer -->
    <script src="bower_components/chosen/chosen.jquery.min.js"></script>
    <!-- plugin for gallery image view -->
